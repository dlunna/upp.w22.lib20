@font-face{
    font-family: graphic-light;
    src: url("https://cdn.hidalgo.gob.mx/fonts/Graphik-Light.otf");
}

@font-face{
    font-family: "Graphik-Bold";
    src: url("https://cdn.hidalgo.gob.mx/fonts/Graphik-Bold.otf");
}

@media (max-width: 1200px) {

  #headergob #headerImagesAnimation{
    height: 110vh !important;
  }

  #headergob .slider__slides,
  #headergob .slider__slide,
  #headergob .slider__slide-parts,
  #headergob .slider__slide-parts:after,
  #headergob .slider__slide-part,
  #headergob .slider__slide-part-inner,
  #headergob .slider__slide-part-inner:before{
    height: 110vh !important;
  }
  #headergob a.nav-link {
      font-size: 11px;
  }

    #headergob #container-search h1{
        text-align: center !important;
        font-size: 22px !important;
        line-height: 35px !important;
    }

    #headergob #container-search input{
        width: calc(100% - 65px) !important;
    }

    #headergob .navbar .gobbrand{
        width: 50% !important;
        display: inline-block !important;
    }

    #headergob .navbar img{
        width: 100% !important;
    }

    #headergob .navbar .custom-toggler{
        float: right !important;
    }

    #headergob .hero-section{
        min-height: 110vh !important;
        margin-top: -110vh !important;
    }

    #headergob .hero-section:before{
        min-height: 110vh !important;
    }

    #headergob #global_search ._box{
        width: 100% !important;
    }

    #headergob .containergob{
        margin-top: 30px !important;
    }

    #headergob .headerphoto_sec{
        width: 50vw !important;
        height: 50vw !important;
        margin-bottom: 50px !important;
        border: 2px solid #fff !important;
    }

    #headergob .containergob .cintilloblanco{
        width: calc(100% - 30px) !important;
        margin-left: 15px !important;
    }

    #headergob .containergob .cintilloazul{
      margin-top: -25px !important;
    }

    #headergob .containergob h5{
      font-size: 20px !important;
    }
    #headergob .containergob b{
      font-size: 15px !important;
    }

    #headergob .containergob .cintilloazul img{
      margin-right: 10px !important;
      margin-top: -5px !important;
    }

    #headergob .hero-banner #container-search .centeredformat {
        margin-top: 50px !important;
    }

}

@media (min-width: 1000px){
  .arrow-down-menu{
    display: none;
  }
}

@media (min-width: 992px) and (max-width: 1199px){

  #headergob #headerImagesAnimation {
    height: 85vh !important;
  }
  #headergob .navbar img{
        width: 80% !important;
  }
  #headergob .navbar .gobbrand{
        width: 30% !important;
  }
  #headergob .containergob{
    margin-top: 0px;
  }
  #headergob .headerphoto_sec{
    width: 20vw !important;
    height: 20vw !important;
  }
  #headergob .hero-banner #container-search{
    margin-top: 200px !important;
  }
}
@media (max-width: 991px){
  #headergob .hero-banner #container-search{
    margin-top: 50px !important;
  }
  #headergob .headerphoto_sec{
    width: 30vw !important;
    height: 30vw !important;
  }
  #headergob .container-gob {
        margin-top: calc(45px + 43px) !important;
  }
  #headergob a.nav-link{
    border-right: none !important;
    display: inline-block !important;
    width: 80% !important;
  }
  #headergob .nav-item{
  border-bottom: 1px solid #fff;
  padding: 10px 0px;
  }
}
@media (max-width: 576px){
  #headergob .hero-banner #container-search{
    margin-top: 10px !important;
  }
  #headergob .headerphoto_sec{
    width: 50vw !important;
    height: 50vw !important;
  }
    #headergob .containergob .cintilloblanco .link {
      font-size: 12px !important;
  }
    #headergob .hero-banner #container-search .centeredformat {
      margin-top: 40px !important;
  }
  #headergob .container-gob {
        margin-top: calc(35px + 43px) !important;
  }
}
