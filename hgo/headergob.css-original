/********************************/

#headergob #buzon{
    display: none;
    z-index: 30;
    position: fixed;
    bottom: 0;
    right: 100px;
    width: 700px;
    height: auto;
    color: #fff;
     box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.14), 0 1px 5px 0 rgba(0, 0, 0, 0.12), 0 3px 1px -2px rgba(0, 0, 0, 0.2);
}
@media (max-width: 1200px){
    #headergob #buzon{
        width: 70%
    }
}
@media (max-width: 1200px){
    #headergob #buzon{
        width: 90%;
        right:0;
        left: 0;
        margin-left: auto;
        margin-right: auto;
        top: 50px;
        height: 70vh;
    }
    #headergob #buzontext{
        height: 80px;
    }
}
#headergob #buzon .x-buzon{
    position: absolute;
    top: 15px;
    right: 15px;
    z-index: 40;
    cursor: pointer;
}
#headergob #buzon .fa-times-circle{
    color: #fff;
    font-size: 25px;
}
#headergob #buzon .fa-envelope-open{
    padding: 5px;
}
#headergob #buzon input textarea{
    border-radius: 12px;
}
#headergob #buzon .form-group{
    margin-bottom: 10px;
}
#headergob .row-buzon{
    height: 100%;
    padding: 10px;
    background-color: var(--azulM);
}
@media (max-width: 1200px){
    #headergob .row-buzon{
        overflow: scroll;
    }
}
#headergob .row-buzon h4{
    margin-left: 10px;
    text-decoration: underline;
    display: inline;
    font-family: "Graphik-Bold" !important;
}
#headergob .row-buzon p{
    padding-top: 5px;
}
#headergob .contacto-info{
    padding: 15px;
}
#headergob a #emailHelp{
    color: #fff;
    text-decoration-color: #fff;
}

#headergob .btn-gobpink{
    color: #fff;
    background-color: var(--rosaM);
    border-color: var(--rosaM);
}
#headergob .btn-gobpink:hover{
    color: #fff;
    background-color: var(--verdeI);
    border-color: var(--verdeI);
    transition: 500ms
}
#headergob #headerImagesAnimation{
  height: 700px;
  background: rgba(8, 72, 102, 1)!important;
}


#headergob .slider {
  overflow: hidden;
  position: relative;
  height: 1200px;
  color: #fff;
}
#headergob .slider__slides {
  position: relative;
  height: 700px;
}
#headergob .slider__slide {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 700px;
  pointer-events: none;
}
#headergob .slider__slide.s--active {
  pointer-events: auto;
}
#headergob .slider__slide-parts {
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  width: 100%;
  height: 700px;
}
#headergob .slider__slide-parts:after {
  content: '';
  z-index: 5;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 700px;
  /*background: rgba(0, 0, 0, 0.05);*/
}
#headergob .slider__slide-part {
  position: relative;
  width: 25%;
  height: 700px;
}
#headergob .slider__slide-part-inner {
  overflow: hidden;
  position: relative;
  width: 100%;
  height: 700px;
  background-size: 0 100%;
  background-repeat: no-repeat;
  transition: -webkit-transform 0.5s ease-in-out;
  transition: transform 0.5s ease-in-out;
  transition: transform 0.5s ease-in-out, -webkit-transform 0.5s ease-in-out;
}
#headergob .slider__slide-part-inner:before {
  content: '';
  position: absolute;
  width: 100vw;
  height: 700px;
  background-image: inherit;
  background-size: cover;
  background-position: center center;
  transition: opacity 0.25s;
  opacity: 0;
}
#headergob .slider__slide-part:nth-child(1) .slider__slide-part-inner {
  z-index: 3;
  transition-delay: 0.24s;
  -webkit-transform: translateX(-32.5%);
          transform: translateX(-32.5%);
}
#headergob .slider__slide.s--active .slider__slide-part:nth-child(1) .slider__slide-part-inner {
  transition-delay: 0.28s;
}
#headergob .slider__slide-part:nth-child(1) .slider__slide-part-inner:before {
  left: 0vw;
  transition-delay: 0.365s;
}
#headergob .slider__slide.s--active .slider__slide-part:nth-child(1) .slider__slide-part-inner:before {
  transition-delay: 0.28s;
}
#headergob .slider__slide-part:nth-child(2) .slider__slide-part-inner {
  z-index: 2;
  transition-delay: 0.16s;
  -webkit-transform: translateX(-65%);
          transform: translateX(-65%);
}
#headergob .slider__slide.s--active .slider__slide-part:nth-child(2) .slider__slide-part-inner {
  transition-delay: 0.36s;
}
#headergob .slider__slide-part:nth-child(2) .slider__slide-part-inner:before {
  left: -25vw;
  transition-delay: 0.285s;
}
#headergob .slider__slide.s--active .slider__slide-part:nth-child(2) .slider__slide-part-inner:before {
  transition-delay: 0.36s;
}
#headergob .slider__slide-part:nth-child(3) .slider__slide-part-inner {
  z-index: 1;
  transition-delay: 0.08s;
  -webkit-transform: translateX(-97.5%);
          transform: translateX(-97.5%);
}
#headergob .slider__slide.s--active .slider__slide-part:nth-child(3) .slider__slide-part-inner {
  transition-delay: 0.44s;
}
#headergob .slider__slide-part:nth-child(3) .slider__slide-part-inner:before {
  left: -50vw;
  transition-delay: 0.205s;
}
#headergob .slider__slide.s--active .slider__slide-part:nth-child(3) .slider__slide-part-inner:before {
  transition-delay: 0.44s;
}
#headergob .slider__slide-part:nth-child(4) .slider__slide-part-inner {
  z-index: 0;
  transition-delay: 0s;
  -webkit-transform: translateX(-130%);
          transform: translateX(-130%);
}
#headergob .slider__slide.s--active .slider__slide-part:nth-child(4) .slider__slide-part-inner {
  transition-delay: 0.52s;
}
#headergob .slider__slide-part:nth-child(4) .slider__slide-part-inner:before {
  left: -75vw;
  transition-delay: 0.125s;
}
#headergob .slider__slide.s--active .slider__slide-part:nth-child(4) .slider__slide-part-inner:before {
  transition-delay: 0.52s;
}
#headergob .slider__slide.s--active .slider__slide-part-inner {
  -webkit-transform: translateX(0);
          transform: translateX(0);
  transition-timing-function: ease;
}
#headergob .slider__slide.s--active .slider__slide-part-inner:before {
  opacity: 1;
}
#headergob .icon-link {
  z-index: 100;
  position: absolute;
  left: 5px;
  bottom: 5px;
  width: 32px;
}
#headergob .icon-link img {
  width: 100%;
  vertical-align: top;
}
#headergob .icon-link--twitter {
  left: auto;
  right: 5px;
}
@media (min-width: 1200px) and (max-height: 750px){
  #headergob #headerImagesAnimation{
    height: 550px ;
