#headergob a{
    cursor: pointer;
}
#headergob .gobbrand{
    width: 25%;
    height: auto;
}
#headergob hr{
    width: 100%;
    border: 1px solid #FFF;
    border-bottom: 0px;
    border-left: 0px;
    border-right: 0px;
    margin-top: 5px;
    margin-bottom: 5px;
}
#headergob hr:after{
    content: " ";
    position: absolute;
    left: 15px;
    border: 0px;
    background-color: #FFF;
    height: 5px;
    width: 25px;
}

:root{
